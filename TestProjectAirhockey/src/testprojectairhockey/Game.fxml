<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.canvas.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" fx:id="stage" onKeyPressed="#keyEventPressed" prefHeight="750.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="testprojectairhockey.GameController">
    <children>
        <Label fx:id="lblPlayer1" layoutX="23.0" layoutY="49.0" minHeight="16" minWidth="69" text="Red:" />
        <Canvas fx:id="canvas" height="498.0" layoutX="100.0" layoutY="55.0" visible="false" width="600.0" />
        <TextField fx:id="tfMessage" focusTraversable="false" layoutX="100.0" layoutY="701.0" onKeyPressed="#enterPressed" prefHeight="26.0" prefWidth="532.0" promptText="Write a message..." />
        <Button fx:id="btnSend" focusTraversable="false" layoutX="643.0" layoutY="701.0" mnemonicParsing="false" onAction="#btnSend_Click" prefHeight="26.0" prefWidth="57.0" text="Send" />
        <ListView fx:id="lvChat" editable="true" layoutX="100.0" layoutY="561.0" prefHeight="129.0" prefWidth="600.0" />
        <Button fx:id="btnExit" focusTraversable="false" layoutX="706.0" layoutY="29.0" mnemonicParsing="false" onAction="#btnExit_Click" text="Exit Game" />
        <Label fx:id="lblPlayer2" layoutX="23.0" layoutY="68.0" minHeight="16" minWidth="69" text="Green:" />
        <Label fx:id="lblPlayer3" layoutX="23.0" layoutY="86.0" minHeight="16" minWidth="69" text="Blue:" />
        <Label fx:id="lblScore3" layoutX="105.0" layoutY="86.0" minHeight="16" minWidth="69" text="20" />
        <Label fx:id="lblScore2" layoutX="105.0" layoutY="68.0" minHeight="16" minWidth="69" text="20" />
        <Label fx:id="lblScore1" layoutX="105.0" layoutY="50.0" minHeight="16" minWidth="69" text="20" />
        <Label fx:id="label3" layoutX="23.0" layoutY="21.0" minHeight="16" minWidth="69" text="Score:">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label layoutX="367.0" layoutY="21.0" text="Round:" textAlignment="CENTER">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <Label fx:id="lblRoundNr" layoutX="444.0" layoutY="21.0" text="1" textAlignment="CENTER">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
      <Button fx:id="btnStart" layoutX="314.0" layoutY="285.0" mnemonicParsing="false" onAction="#btnStart_Click" prefHeight="110.0" prefWidth="239.0" text="Start Game" />
      <Label fx:id="lblWaiting" layoutX="383.0" layoutY="224.0" text="Waiting for players..." textAlignment="CENTER" />
    </children>
</AnchorPane>
